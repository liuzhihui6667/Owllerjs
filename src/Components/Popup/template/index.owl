<div class="owl-pop-container ' +animation+ '" id="' +this.$hashKey+ '">
    <div class="owl-pop-title-wrapper">
            <span class="owl-pop-title">'+this.$title+'</span>
            <svg width="15px" height="15px" viewBox="0 0 15 15" class="owl-pop-close">
                    <path d="M0,0 L15,15 M0,15 L15, 0" stroke="#666666" stroke-width="2px" stroke-opacity="0.7"></path>
                </svg>
        </div>
    <div class="owl-pop-body-wrapper">
            <p class="owl-pop-text">'+ this.$text +'</p>
            <div class="owl-pop-button confirm">确定</div>
            <div class="owl-pop-button cancel">取消</div>
        </div>
    </div>